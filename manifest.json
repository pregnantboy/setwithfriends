{
  "name": "Upset your friends  - Set with friends hack",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Win games, lose friends. Spacebar to highlight; m to autoclick.",
  "homepage_url": "https://setwithfriends.com",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["https://setwithfriends.com/*"],
  "content_scripts": [
    {
      "matches": ["https://setwithfriends.com/*"],
      "js": ["src/inject/inject.js"],
      "css": ["src/inject/inject.css"],
      "run_at": "document_end"
    }
  ]
}
